{
  "manifest_version": 2,
  "name": "RSSLater",
  "version": "0.0.0.1",
  "permissions": ["activeTab", "storage"],
  "content_security_policy": "script-src 'self'; object-src 'none'",
  "externally_connectable": {
    "matches": ["https://*/*"]
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
